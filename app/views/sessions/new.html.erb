<h1>Sessions#new</h1>
<p>Find me in app/views/sessions/new.html.erb</p>


<h1>Sign In</h1>
<%= form_with url: sign_in_path do |f| %>
  <p>
    <%= f.label 'username:' %><br>
    <%= f.text_field :username, id: 'username' %>
  </p>
  <p>
    <%= f.label 'password:' %><br>
    <%= f.password_field :password, id: 'password' %>
  </p>
  <p>
    <%= f.submit 'Log In' %>
  </p>
<% end %>

<%= link_to 'Sign Up', sign_up_path %>
