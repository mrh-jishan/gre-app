<h1>Passwords#edit</h1>
<p>Find me in app/views/passwords/edit.html.erb</p>


<h1>Edit Password</h1>
<%= form_with model: @user, url: edit_password_path do |f| %>
  <p>
    <%= f.label 'password:'%><br>
    <%= f.password_field :password %>
  </p>
  <p>
    <%= f.label 'password_confirmation:'%><br>
    <%= f.password_field :password_confirmation %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>